{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.15.0",
  "scripts": {
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPatterns=__tests__/(components|hooks|lib)",
    "test:integration": "jest --testPathPatterns=__tests__/integration",
    "test:e2e": "jest --testPathPatterns=__tests__/e2e",
    "test:e2e:auth": "jest --testPathPatterns=__tests__/e2e/auth-flow.e2e.test.tsx",
    "test:e2e:tenant": "jest --testPathPatterns=__tests__/e2e/tenant-isolation.e2e.test.tsx",
    "test:e2e:business": "jest --testPathPatterns=__tests__/e2e/business-flows.e2e.test.tsx",
    "test:e2e:all": "node __tests__/e2e/run-e2e-tests.js --category all",
    "test:e2e:dev": "node __tests__/e2e/run-e2e-tests.js --env development --category all",
    "test:e2e:staging": "node __tests__/e2e/run-e2e-tests.js --env staging --category all",
    "test:e2e:coverage": "node __tests__/e2e/run-e2e-tests.js --category all --coverage",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "perf:analyze": "npm run build:analyze",
    "perf:lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html"
  },
